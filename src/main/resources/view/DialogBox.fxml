        <Label fx:id="dialog" text="Label" wrapText="true" minHeight="-Infinity" maxWidth="320" />
        <ImageView fx:id="displayPicture" fitHeight="40" fitWidth="40" pickOnBounds="true" preserveRatio="true" />
